      "dependencies": ["@react-email/components", "react", "react-dom"],
      "files": [
        {
          "path": "libraries/react-email.tsx",
          "type": "registry:file",
          "target": "email/my-email.tsx"
        }
      ]
    },
    {
      "name": "prisma",
