{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "zero",
  "homepage": "http://localhost:3000",
  "items": [
    {
      "name": "prisma",
      "type": "registry:block",
      "title": "Prisma",
      "description": "Prisma is a powerful ORM for TypeScript and Node.js.",
      "dependencies": ["prisma"],
      "files": [
        {
          "path": "recipes/prisma.ts",
          "type": "registry:lib",
          "target": "lib/prisma.ts"
        },
        {
          "path": "recipes/schema.prisma",
          "type": "registry:file",
          "target": "prisma/schema.prisma"
        }
      ]
    },
    {
      "name": "twilio",
      "type": "registry:block",
      "title": "Twilio",
      "description": "Twilio is a powerful SMS and voice API for TypeScript and Node.js.",
      "dependencies": ["twilio"],
      "files": [
        {
          "path": "recipes/twilio.ts",
          "type": "registry:lib",
          "target": "lib/twilio.ts"
        }
      ]
    },
    {
      "name": "resend",
      "type": "registry:block",
      "title": "Resend",
      "description": "Resend is a powerful email API for TypeScript and Node.js.",
      "dependencies": ["resend"],
      "files": [
        {
          "path": "recipes/resend.ts",
          "type": "registry:lib",
          "target": "lib/resend.ts"
        }
      ]
    },
    {
      "name": "better-auth-next-pages",
      "type": "registry:block",
      "title": "Better Auth Next Pages",
      "description": "Better Auth Next Pages is a powerful authentication library for Next.js.",
      "dependencies": ["better-auth"],
      "files": [
        {
          "path": "recipes/better-auth/better-auth.ts",
          "type": "registry:lib",
          "target": "lib/auth.ts"
        },
        {
          "path": "recipes/better-auth/better-auth-client.ts",
          "type": "registry:lib",
          "target": "lib/auth-client.ts"
        },
        {
          "path": "recipes/better-auth/better-auth-pages-handler.ts",
          "type": "registry:page",
          "target": "pages/api/auth/[...all].ts"
        }
      ]
    }
  ]
}
